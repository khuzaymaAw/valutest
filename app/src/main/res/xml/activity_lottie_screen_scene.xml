<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <OnSwipe
            motion:touchAnchorId="@+id/lottie"
            motion:touchAnchorSide="middle"
            motion:dragDirection="dragUp" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
        android:id="@+id/lottie"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:visibility="visible"
        motion:layout_constraintBottom_toBottomOf="parent"
        motion:layout_constraintLeft_toLeftOf="parent"
        motion:layout_constraintRight_toRightOf="parent"
        motion:layout_constraintTop_toTopOf="parent"
        motion:motionProgress="0">
        </Constraint>
        <Constraint
            motion:layout_constraintVertical_bias="0.082"
            android:layout_height="63.3dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.913"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="63.3dp"
            android:id="@+id/hamburger_btn" />
        <Constraint
            motion:layout_constraintVertical_bias="0.923"
            android:layout_height="63.3dp"
            motion:layout_constraintEnd_toStartOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.2"
            android:layout_width="63.3dp"
            android:layout_marginStart="28dp"
            android:id="@+id/reserve_btn"
            android:layout_marginLeft="28dp" />
        <Constraint
            motion:layout_constraintVertical_bias="0.901"
            android:layout_height="21dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="73dp"
            android:id="@+id/connect_btn" />
        <Constraint
            motion:layout_constraintVertical_bias="0.923"
            android:layout_height="63.3dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.913"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="63.3dp"
            android:id="@+id/use_btn" />
        <Constraint
            motion:layout_constraintVertical_bias="0.082"
            android:layout_height="63.3dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.086"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="63.3dp"
            android:id="@+id/ring_btn" />

    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">

    <Constraint
        android:id="@+id/lottie"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:visibility="gone"
        motion:layout_constraintLeft_toLeftOf="parent"
        motion:layout_constraintRight_toRightOf="parent"
        motion:layout_constraintTop_toTopOf="parent"
        motion:motionProgress="1">

    </Constraint>
        <Constraint
            motion:layout_constraintVertical_bias="0.082"
            android:layout_height="63.3dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.913"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="63.3dp"
            android:id="@+id/hamburger_btn" />
        <Constraint
            motion:layout_constraintVertical_bias="0.923"
            android:layout_height="63.3dp"
            motion:layout_constraintEnd_toStartOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.2"
            android:layout_width="63.3dp"
            android:layout_marginStart="28dp"
            android:id="@+id/reserve_btn"
            android:layout_marginLeft="28dp" />
        <Constraint
            motion:layout_constraintVertical_bias="0.901"
            android:layout_height="21dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="73dp"
            android:id="@+id/connect_btn" />
        <Constraint
            motion:layout_constraintVertical_bias="0.923"
            android:layout_height="63.3dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.913"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="63.3dp"
            android:id="@+id/use_btn" />
        <Constraint
            motion:layout_constraintVertical_bias="0.082"
            android:layout_height="63.3dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintHorizontal_bias="0.086"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="63.3dp"
            android:id="@+id/ring_btn" />

    </ConstraintSet>
</MotionScene>
